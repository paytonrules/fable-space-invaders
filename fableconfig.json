{
  "sourceMaps": true,
  "targets": {
    "umd": {
      "projFile": [
        "./SpaceInvaders/SpaceInvaders.fsproj",
        "./Browser/Browser.fsproj"
      ],
      "rollup": {
        "dest": "./dist/umd/Main.js",
        "format": "umd"
      }
    },
    "test": {
      "projFile": [
        "./SpaceInvaders/SpaceInvaders.fsproj",
        "./Browser/Browser.fsproj",
        "./test/Test.fsproj"
      ],
      "outDir": "./dist/test",
      "module": "commonjs",
      "coreLib": "fable-core/umd",
      "plugins": ["./node_modules/fable-plugins-nunit/Fable.Plugins.NUnit.dll"],
      "scripts": {
        "postbuild": "mocha --reporter spec --colors --sort --recursive ./dist/test/test/fs"
      }
    }
  }
}
